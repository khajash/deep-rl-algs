{
  "env": "LunarLander-v2",
  "env_params": {
    "continuous": false,
    "gravity" : -10.0,
    "enable_wind" : false,
    "wind_power" : 15.0,
    "turbulence_power" : 1.5
  },
  "optimizer": "adam",
  "optim_params": {
    "lr": 1e-3
  },
  "network": "DQN",
  "policy": "linear",
  "policy_params": {
    "in_features": 8,
    "lin_layers": [128, 128],
    "use_bn": false
  },
  "alg_params": {
    "explore_kwargs": {
      "decay": "linear",
      "val": 1.0,
      "end_val": 0.1,
      "duration": 1e5
    },
    "target_update": 100,
    "mem_size":1e6,
    "batch_size": 64,
    "gamma": 0.998
  }
}
